## Rastrear el rat√≥n

fill(255, 0, 255);

draw = function() {
    background(255, 255, 255);
    var X = mouseX;
    var Y = mouseY;
    var cord = X + "," + Y;
    textSize(20);
    text(cord, mouseX+10, mouseY);
    ellipse(mouseX, mouseY, 12, 12);
    
};
